		14 |       containers:
		15 |       - name: health-check
		16 |         image: madhuakula/k8s-goat-health-check
		17 |         resources:
		18 |           limits:
		19 |             memory: "100Mi"
		20 |             cpu: "30m"
		21 |         ports:
		22 |         - containerPort: 80
		23 |       # Custom Stuff
		24 |         securityContext:
		25 |           privileged: true
		26 |         volumeMounts:
		27 |           - mountPath: /custom/docker/docker.sock
		28 |             name: docker-sock-volume
		29 |       volumes:
		30 |         - name: docker-sock-volume
